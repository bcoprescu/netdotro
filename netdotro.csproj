    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>ccdea44f-adb1-465c-badf-39ef8aad792f</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <None Update="app.db" CopyToOutputDirectory="PreserveNewest" ExcludeFromSingleFile="true" />
